{
  "name": "angular-mobile-app",
  "version": "0.0.1",
  "scripts": {
    "start": "ng serve",
    "serve": "ng serve --host=0.0.0.0 --disable-host-check",
    "build": "ng build",
    "* build-pr": "ng build angular-mobile-app -c production  --source-map",
    "init capacitor": "npx cap init angular-mobile-app com.remotervlc.angularmobileapp",
    "init ionic": "npx ionic init angular-mobile-app --type=angular",
    "serve ionic": "npx ionic serve",
    "run": "ng run angular-mobile-app:serve --host=localhost --port=8100",
    "ionic enable cap": "npx ionic integrations enable capacitor",
    "ionic cap add android": "npx ionic capacitor add android",
    "* ionic cap build": "ionic capacitor build android --prod",
    "add android": "npx cap add android",
    "add ois": "npx cap add ois",
    "* sync-cap": "npx cap sync",
    "* open-android": "npx cap open android",
    "watch VLC": "ng build --watch --configuration   --output-path www --watch --output-hashing none",
    "static-2django": "ng build  --configuration production  --output-path ../backend/static/ang --watch --output-hashing none",
    "ng": "ng",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^15.1.4",
    "@angular/animations": "15.1.3",
    "@angular/common": "15.1.3",
    "@angular/compiler": "^15.1.3",
    "@angular/core": "^15.1.3",
    "@angular/forms": "^15.1",
    "@angular/platform-browser": "15.1.3",
    "@angular/platform-browser-dynamic": "15.1.3",
    "@angular/router": "^15.1.3",
    "@capacitor/android": "4.6.3",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "4.1.1",
    "@capacitor/browser": "^4.1.1",
    "@capacitor/camera": "^4.1.4",
    "@capacitor/core": "4.6.3",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.6.3",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/network": "^4.1.0",
    "@capacitor/status-bar": "4.1.1",
    "@ionic-native/network": "^5.36.0",
    "@ionic/angular": "^6.5.2",
    "@ionic/pwa-elements": "^3.1.1",
    "bootstrap": "^5.2.1",
    "bootstrap-icons": "^1.9.1",
    "jquery": "^3.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.4",
    "@angular/cli": "^15.1.4",
    "@angular/compiler-cli": "^15.1.3",
    "@angular/core": "^15.1.3",
    "@angular/platform-browser-dynamic": "^15.1.3",
    "@angular/router": "^15.1.3",
    "@capacitor/cli": "4.6.3",
    "@ionic/angular-toolkit": "^7.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.4.0",
    "@types/jquery": "^3.5.14",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "^4.9.5"
  }
}
