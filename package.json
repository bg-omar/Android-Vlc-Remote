{
  "name": "vlc-remote",
  "version": "0.0.1",
  "author": "Iskandarani",
  "homepage": "https://omariskandarani.com/",
  "scripts": {
    "* serve": "npm run copy:sql:wasm && ng serve --host=0.0.0.0 --disable-host-check",
    "* serve ionic": "npx ionic serve",
    "* build-pr": "ng build vlc-remote -c production  --source-map",
    "* build-webpack": "webpack --mode=production",
    "* sync-cap": "npx cap sync",
    "* open-android": "npx cap open android",
    "* ionic cap build": "ionic capacitor build android --prod",
    "* copy-cap": "npx cap copy",
    "* run-android": "npm run ionic:android",
    "start-webpack": "webpack-dev-server --mode=development --open",
    "serve json": "json-server db.json ",
    "build:web": "npm run copy:sql:wasm && ng build",
    "build:native": "npm run remove:sql:wasm && ng build",
    "init capacitor": "npx cap init vlc-remote com.omariskandarani.vlcremote",
    "init ionic": "npx ionic init vlc-remote --type=angular",
    "run": "ng run vlc-remote:serve --host=localhost --port=8100",
    "watch VLC": "ng build --watch --configuration   --output-path www --watch --output-hashing none",
    "watch": "npm run copy:sql:wasm && ng build --watch --configuration development",
    "ng": "ng",
    "test": "ng test",
    "ionic:serve:before": "npm run copy:sql:wasm",
    "copy:sql:wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm src/assets",
    "remove:sql:wasm": "rimraf src/assets/sql-wasm.wasm",
    "ionic:ios": "npm run remove:sql:wasm && ionic capacitor build ios",
    "ionic:android": "npm run remove:sql:wasm && ionic capacitor build android",
    "ionic enable cap": "npx ionic integrations enable capacitor",
    "ionic cap add android": "npx ionic capacitor add android",
    "add android": "npx cap add android",
    "add ois": "npx cap add ois"
  },
  "private": true,
